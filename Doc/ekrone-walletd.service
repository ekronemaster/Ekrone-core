[Unit]
Description=Ekrone Infinity Walletd
After=ekroned.service

[Service]
User=YOUR_USER
Group=YOUR_USER

WorkingDirectory=/home/YOUR_USER/.ekrone
ExecStart=/usr/bin/ekrone-walletd --data-dir /home/YOUR_USER/.ekrone --rpc-user RPC_USER --rpc-password RPC_PASSWORD -w /path/to/wallet/file -p PASSWORD --daemon-port 43000
GuessMainPID=no

[Install]
WantedBy=multi-user.target
